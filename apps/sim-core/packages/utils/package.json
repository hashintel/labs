{
  "name": "@hashintel/utils",
  "version": "0.0.1",
  "private": true,
  "description": "A collection of utils for use in HASH projects",
  "scripts": {
    "prepublishOnly": "tsc -b .",
    "postinstall": "tsc -b .",
    "build": "tsc -b .",
    "build:watch": "tsc -b . -w",
    "fmt": "prettier --cache \"*.{ts,js,json}\" \"{scripts,src}/**/*.{ts,js,json}\" --write; eslint --quiet --fix \"*.{ts,js,json}\" \"{scripts,src}/**/*.{ts,js,json}\"",
    "lint": "prettier --cache \"*.{ts,js,json}\" \"{scripts,src}/**/*.{ts,js,json}\" --check || exit 1; eslint --quiet \"*.{ts,js,json}\" \"{scripts,src}/**/*.{ts,js,json}\" ; tsc --noEmit"
  },
  "license": "SSPL-1.0",
  "devDependencies": {
    "@types/node": "^14.14.7",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@types/papaparse": "5.2.5",
    "papaparse": "5.3.0"
  }
}
